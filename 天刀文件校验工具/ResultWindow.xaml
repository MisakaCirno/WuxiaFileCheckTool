<Window x:Class="天刀客户端自助修复工具.ResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:天刀客户端自助修复工具"

        mc:Ignorable="d"
        Title="文件对比结果" Height="450" Width="800">

    <Grid>
        <ListView x:Name="main_ListView" FontSize="14">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="文件状态" DisplayMemberBinding="{Binding State}" />
                    <GridViewColumn Header="文件路径" DisplayMemberBinding="{Binding FilePath}" />
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>